{
  "name": "docbook",
  "version": "0.0.1",
  "description": "Static documentation generator",
  "main": "build/index.js",
  "scripts": {
    "babel": "babel ./src --out-dir build/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "start": "node build/",
    "build": "npm run babel && node build/"
  },
  "bin": {
    "docbook": "bin/docbook"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/blenderskool/docbook"
  },
  "author": "Akash Hamirwasia",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-minify": "^0.4.3"
  },
  "dependencies": {
    "clean-css": "^4.1.11",
    "dom-parser": "^0.1.5",
    "html-minifier": "^3.5.16",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "showdown": "^1.8.6"
  }
}
